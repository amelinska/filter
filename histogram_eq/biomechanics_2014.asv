%biomechanics_2014

load s100;
original_image = s100;
original_image = (original_image/max(original_image(:)));

[diff, original_bone, struct_of_bones_before, struct_of_bones_after]  = compare_images(original_image,1);

image_equaled = histeq(original_image); 
image_equaled_diff = compare_images(image_equaled);


LOW_HIGH = stretchlim(original_image);
image_stretched = imadjust(original_image,LOW_HIGH);
image_stretched_diff = compare_images(image_stretched);